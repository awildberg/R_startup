# convert 3M or 3K to 3000000 and 3000
convSuffix <- function(str_vec){

  m <- grep("m|M",str_vec);
  k <- grep("k|K",str_vec);
  z <- grep("[0-9]$",str_vec);
  str_vec <- gsub("[^0-9.-]","",str_vec);
  tmp[c(m,k,z)] <- c( paste0(str_vec[m],"e6"), paste0(str_vec[k],"e3"), str_vec[z] );
  as.numeric(tmp)
  
}
